generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Post {
  id            Int       @id @default(autoincrement())
  subject       String    @unique
  content       String
  contentHtml   String?
  isPublished   Boolean   @default(false)
  createdAt     DateTime  @default(now())
  publishedAt   DateTime?
}
